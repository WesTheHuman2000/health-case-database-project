    <h1>Edit Patient</h1>
    <form id="editPatientForm" action="/edit/patient" method="post" >
        <input type="hidden" name="patientId" value="<%= patientData.PatientId %>">
        
        <select id="newInsId" name="newInsId" class="form-select" name="">
          <option value="UUI2536584">UUI2536584</option>
          <option value="UUI2722096">UUI2722096</option>
          <option value="UUI3182647">UUI3182647</option>
          <option value="UUI320287">UUI320287</option>
          <option value="UUI3231448">UUI3231448</option>
          <option value="UUI3300800">UUI3300800</option>
          <option value="UUI3905062">UUI3905062</option>
          <option value="UUI4286889">UUI4286889</option>
          <option value="UUI4543246">UUI4543246</option>
          <option value="UUI4560719">UUI4560719</option>
          <option value="UUI4633330">UUI4633330</option>
          <option value="UUI4654820">UUI4654820</option>
          <option value="UUI4716824">UUI4716824</option>
          <option value="UUI4807415">UUI4807415</option>
      </select>

        <div class="mb-3">
          <label for="newAddress" class="form-label">Address</label>
          <input type="text" name="newAddress" id="newAddress" value="<%= patientData.Address1 %>">
        </div>

        <div class="mb-3">
          <label for="newZip" class="form-label">Zip</label>
          <input type="text" name="newZip" id="newZip" value="<%= patientData.ZipCode %>">
        </div>
        
        <div class="mb-3">
          <label for="newCity" class="form-label">City</label>
          <input type="text" name="newCity" id="newCity" value="<%= patientData.City %>">
        </div>
        <!--
        <div class="mb-3">
          <label for="exampleInputPassword1" class="form-label">Password</label>
          <input type="password" class="form-control" id="exampleInputPassword1">
        </div>    
        -->
        
        <button type="submit" class="btn btn-primary">Submit</button>
      </form>
</body>
</html>